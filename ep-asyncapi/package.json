  "license": "APACHE-2.0",
  "bugs": {
    "url": "https://github.com/solace-labs/ep-asyncapi/issues"
  },
  "scripts": {
    "compile:examples": "tsc --project examples/tsconfig.json",
    "compile": "tsc -b && npm run compile:examples",
    "build:tsdocs": "ts-node build-tsdocs.ts",
    "build": "npm run build:tsdocs && tsc",
    "dev:build": "tsc",
    "test": "npm run compile && . ./test/source.env.sh && nyc mocha --config test/.mocharc.yml test/**/**.spec.ts && unset_source_env"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public",
    "cache": ".cache/.npm",
    "registry": "https://registry.npmjs.org",
    "tag": "latest"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^15.6.1",
    "@types/semver": "^7.3.10",
    "@types/shelljs": "^0.8.9",
    "@types/sinon": "^10.0.12",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "chai": "^4.3.6",
    "deepmerge": "^4.2.2",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "shelljs": "^0.8.4",
    "sinon": "^14.0.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.8.1",
